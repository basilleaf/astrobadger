{
  "manifest_version": 2,
  "name": "AstroBadger",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.0.0.min.js", "content.js"],
      "css": ["style.css"]
    }
  ],
  "browser_action": {
    "default_icon": "badger16.png"
  },
  "icons": {  "16": "badger16.png",
              "48": "badger48.png",
             "128": "badger128.png"}
}
